{
  "family": "ecs-app-node",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "",
      "image":"your-docker-image-repository-url/my-nodejs-app:latest",
      "memory":512,
      "cpu":256,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000
